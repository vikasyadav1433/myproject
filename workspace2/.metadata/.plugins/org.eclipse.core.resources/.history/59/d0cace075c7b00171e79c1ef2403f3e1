package www.vikas.model;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class Product {
	@Id
	private String productid;
	private String productnaem;
	private String productdesc;
	private boolean isproductavailabel;
	private float rating;
	private int noitemssold;
	private int noreviews;
	public String getProductid() {
		return productid;
	}
	public void setProductid(String productid) {
		this.productid = productid;
	}
	public String getProductnaem() {
		return productnaem;
	}
	public void setProductnaem(String productnaem) {
		this.productnaem = productnaem;
	}
	public String getProductdesc() {
		return productdesc;
	}
	public void setProductdesc(String productdesc) {
		this.productdesc = productdesc;
	}
	public boolean isIsproductavailabel() {
		return isproductavailabel;
	}
	public void setIsproductavailabel(boolean isproductavailabel) {
		this.isproductavailabel = isproductavailabel;
	}
	public float getRating() {
		return rating;
	}
	public void setRating(float rating) {
		this.rating = rating;
	}
	public int getNoitemssold() {
		return noitemssold;
	}
	public void setNoitemssold(int noitemssold) {
		this.noitemssold = noitemssold;
	}
	public int getNoreviews() {
		return noreviews;
	}
	public void setNoreviews(int noreviews) {
		this.noreviews = noreviews;
	}
	
	
	

}
